{
  "log_params": {
    "log_directory": "logs",
    "log_info": "info.log",
    "log_error": "error.log",
    "log_warn": "warn.log",
    "log_debug": "debug.log",
    "max_size_megabytes": 10,
    "max_backups": 4,
    "max_age": 30,
    "compress": true,
    "local_time": true
  },
  "app_params": {
    "gin_mode": "debug",
    "port_run": "9090",
    "server_url": "localhost",
    "server_name": "TechnoCredits_service"
  },
  "postgres_params": {
    "host": "localhost",
    "port": "5432",
    "user": "postgres",
    "database": "techno_db",
    "sslmode": "disable"
  },
  "cors": {
    "allow_origins": ["http://localhost:3000"],
    "allow_methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"],
    "allow_headers": ["Origin", "Content-Type", "Authorization"],
    "expose_headers": ["Content-Length"],
    "allow_credentials": true
  },
  "redis_params": {
    "host": "localhost",
    "port": 6379,
    "password": "",
    "db": 0,
    "ttl_minutes": 10
  },
  "rabbit_params": {
    "url_conn": "amqp://guest:guest@localhost:5672/"
  },
  "auth_params": {
    "jwt_secret_key": "",
    "jwt_ttl_minutes": 60
  }
}
